{
  "name": "@prontopro/awesome-react-rating",
  "version": "0.1.21",
  "description": "An awesome React Rating component",
  "main": "dist/index.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist/",
    "src/"
  ],
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProntoPro/awesome-react-rating.git"
  },
  "moduleRoots": [
    "",
    "src"
  ],
  "keywords": [
    "rating",
    "react",
    "component",
    "react-component"
  ],
  "author": "ProntoPro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ProntoPro/awesome-react-rating/issues"
  },
  "homepage": "https://github.com/ProntoPro/awesome-react-rating",
  "jest": {
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/utils/**/*.js",
      "!src/**/__stories__/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 73,
        "functions": 75,
        "lines": 72,
        "statements": 72
      }
    }
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.37.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "flow-bin": "^0.61.0",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts": "^1.0.14",
    "react-test-renderer": "^16.0.0",
    "sync-files": "^1.0.3",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-tap-event-plugin": "^3.0.2",
    "styled-components": "^2.2.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress --hot",
    "eslint:watch": "esw -w --clear --cache ./src",
    "eslint": "clear && eslint ./src && echo \"No errors found.\"",
    "start": "react-scripts start",
    "storybook": "NODE_PATH=src start-storybook -p 9009 -s public",
    "storybook:build": "mkdir -p dist && NODE_PATH=src build-storybook -c .storybook -o dist/storybook",
    "test:ci": "yarn test -- --colors --lastCommit --runInBand --silent",
    "test:watch": "yarn test -- --watch",
    "test": "react-scripts test --coverage --env=jsdom"
  }
}
